{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "bin": {
    "clog-ai": "dist/index.js"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "ai-commit-log",
  "author": "yutou-air",
  "module": "dist/ai-commit-log.esm.js",
  "size-limit": [
    {
      "path": "dist/ai-commit-log.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/ai-commit-log.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "axios": "^1.6.2",
    "execa": "^8.0.1",
    "loading-cli": "^1.1.2",
    "prettier": "^3.1.1",
    "tsdx": "^0.14.1"
  }
}
